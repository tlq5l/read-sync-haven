{
	"extends": "./tsconfig.app.json",
	"include": ["src/**/*"],
	"exclude": ["**/node_modules", "**/.*/", "**/*.test.ts", "**/*.test.tsx"],
	"compilerOptions": {
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"target": "ES2022",
		"composite": false,
		"allowImportingTsExtensions": true,
		"noEmit": true
	}
}
